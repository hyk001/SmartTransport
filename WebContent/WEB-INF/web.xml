<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>ITSystem</display-name>
   <servlet>
     <servlet-name>CongestionIndex</servlet-name>
     <servlet-class>com.its.servlet.ITSServlet.CongestionIndex</servlet-class>
   </servlet>
   <servlet>
     <servlet-name>HistoryConIndex</servlet-name>
     <servlet-class>com.its.servlet.ITSServlet.HistoryConIndex</servlet-class>
   </servlet>
   <servlet>
     <servlet-name>RoadCongestion</servlet-name>
     <servlet-class>com.its.servlet.ITSServlet.RoadCongestion</servlet-class>
   </servlet>
  <servlet-mapping>
    <servlet-name>HistoryConIndex</servlet-name>
    <url-pattern>/HistoryCongestion</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CongestionIndex</servlet-name>
    <url-pattern>/Congestion</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RoadCongestion</servlet-name>
    <url-pattern>/RoadCongestionState</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>